{
  "expo": {
    "name": "Rhythm√©",
    "slug": "rhythme-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/R.png",
    "scheme": "rhythme",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rhythme.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0F",
        "foregroundImage": "./assets/images/R.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.rhythmemobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/R.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/R.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#0A0A0F",
          "dark": {
            "image": "./assets/images/R.png",
            "backgroundColor": "#0A0A0F"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
